{
  "name": "performanceboard",
  "version": "0.0.0",
  "main": "gulpfile.js",
  "devDependencies": {
    "bootstrap": "~3.2.0",
    "brfs": "~1.2.0",
    "browserify": "~4.2.3",
    "browserify-shim": "~3.6.0",
    "gulp": "~3.8.6",
    "gulp-bower": "0.0.6",
    "gulp-concat": "^2.2.0",
    "gulp-jshint": "~1.8.4",
    "gulp-minify-css": "^0.3.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "~0.3.1",
    "vinyl-source-stream": "~0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mgbelisle/performanceboard.git"
  },
  "author": "mgbelisle@gmail.com",
  "bugs": {
    "url": "https://github.com/mgbelisle/performanceboard/issues"
  },
  "homepage": "http://performanceboard.appspot.com/",
  "browserify": {
    "transform": [
      "brfs",
      "browserify-shim"
    ]
  },
  "browser": {
    "angular": "./static/bower_components/angular/angular.js",
    "angular-route": "./static/bower_components/angular-route/angular-route.js",
    "bootstrap": "./static/bower_components/bootstrap/dist/js/bootstrap.js",
    "jquery": "./static/bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery"
      ]
    },
    "angular-route": {
      "depends": [
        "angular"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery"
      ]
    },
    "jquery": "$"
  }
}
